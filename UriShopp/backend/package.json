{
  "name": "urishop-backend",
  "version": "1.0.0",
  "description": "Backend para UriShop ecommerce con MongoDB",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node config/seeder.js seed",
    "seed:clear": "node config/seeder.js clear",
    "seed:reset": "node config/seeder.js reset",
    "load:json": "node scripts/loadJsonToMongo.js",
    "load:productos": "node scripts/loadJsonToMongo.js ./productos.json ./carts.json",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.0",
    "multer": "^1.4.5",
    "cloudinary": "^1.37.3",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.7.0",
    "joi": "^17.9.0",
    "stripe": "^12.0.0",
    "socket.io": "^4.6.0",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "dotenv": "^16.1.4",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0",
    "express-async-handler": "^1.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "jest": "^29.5.0",
    "eslint": "^8.42.0",
    "eslint-config-node": "^4.1.0"
  },
  "keywords": [
    "ecommerce", 
    "node", 
    "express", 
    "mongodb", 
    "mongoose",
    "api",
    "rest"
  ],
